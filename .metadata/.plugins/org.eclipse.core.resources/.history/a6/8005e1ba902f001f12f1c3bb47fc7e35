package Inheritence;

class A{
	void m1() {
		System.out.println("Inside Parent class");
	}
}

class B extends A{
	void m2() {
		System.out.println("Inside child class");
	}
}

class C extends B{
	void m3() {
		System.out.println("Inside grand sub class");
	}
}
public class MultiLevelInheritence {

	public static void main(String[] args) {
		C obj = new C();
		obj.m1();
		obj.m2();
		obj.m3();	

	}

}
